{
    "_id": "_design/directories",
    "kazoo": {
        "view_map": [
            {
                "classification": "account"
            }
        ]
    },
    "language": "javascript",
    "views": {
        "users_listing": {
            "map": [
                "function(doc) {",
                "  if (doc.pvt_deleted || typeof doc.directories !== 'object') return;",
                "  for (directory_id in doc.directories) {",
                "    emit([directory_id, doc._id], doc.directories[directory_id]);",
                "  }",
                "}"
            ]
        }
    }
}
