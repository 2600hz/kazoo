{
    "_id": "_design/views",
    "kazoo": {
        "view_map": [
            {
                "database": "system_data"
            }
        ]
    },
    "language": "javascript",
    "views": {
        "views_by_classification": {
            "map": "function(doc) {if (doc.pvt_type != 'view_definition' || doc.pvt_deleted) return; emit(doc.classification, null);}"
        }
    }
}
