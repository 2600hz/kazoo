{
    "_id": "_design/token_auth",
    "language": "javascript",
    "views": {
        "listing_by_ctime": {
            "map": "function(doc) { emit(doc.created || doc.pvt_created, doc._rev); }"
        },
        "listing_by_mtime": {
            "map": "function(doc) { emit(doc.modified || doc.pvt_modified, doc._rev); }"
        },
        "refresh_tokens_listing": {
            "map": "function(doc) { if (doc.pvt_type != 'refresh_token' || doc.pvt_deleted) return; emit([doc.account_id, doc.owner_id], doc._id); }"
        }
    }
}
