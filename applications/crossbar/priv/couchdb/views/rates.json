{ "_id":"_design/rates"
  ,"language":"javascript"
  ,"views":{
    "lookup": {
      "map": "function(doc) { if (doc.deleted || doc.pvt_type !== 'rate') return; emit(parseInt(doc.prefix), null); }"
    },
    "crossbar_listing": {
      "map":"function(doc) { if (doc.deleted || doc.pvt_type !== 'rate') return; emit(doc._id, {\"name\":doc.rate_name, \"cost\":doc.rate_cost, \"carrier\":doc.carrier}); }"
    }
  }
}
