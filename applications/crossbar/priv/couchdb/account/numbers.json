{
    "_id": "_design/numbers",
    "filters": {
        "export": "function(doc, req) { return ( doc.pvt_type == 'number' ); }"
    },
    "language": "javascript",
    "views": {
        "list_by_number": {
            "map": "function(doc) {if (doc.pvt_type != 'number' || doc.pvt_deleted) return;emit(doc._id, null);}"
        }
    }
}
