        "Application-Name": {
            "type": "string"
        },
        "Args": {
            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "fs"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "command"
            ],
            "type": "string"
        },
        "Insert-At": {
            "type": "string"
        }
    },
    "required": [
        "Application-Name",
        "Args"
    ],
    "type": "object"
}
