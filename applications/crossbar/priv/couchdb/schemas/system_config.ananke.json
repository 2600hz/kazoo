{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "_id": "system_config.ananke",
    "description": "Schema for ananke system_config",
    "properties": {
        "schedules": {
            "default": [],
            "description": "ananke schedules",
            "type": "array"
        },
        "voicemail": {
            "properties": {
                "notify": {
                    "properties": {
                        "callback": {
                            "properties": {
                                "attempts": {
                                    "properties": {
                                        "default": 5,
                                        "description": "ananke attempts",
                                        "type": "string"
                                    }
                                },
                                "interval_s": {
                                    "properties": {
                                        "default": 300,
                                        "description": "ananke interval in seconds",
                                        "type": "string"
                                    }
                                },
                                "timeout_s": {
                                    "properties": {
                                        "default": 20,
                                        "description": "ananke timeout in seconds",
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "required": true,
    "type": "object"
}
