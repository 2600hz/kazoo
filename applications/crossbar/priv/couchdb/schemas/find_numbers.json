{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "find_numbers",
    "properties": {
        "country": {
            "default": "US",
            "maxLength": 2,
            "minLength": 2,
            "type": "string"
        },
        "prefix": {
            "maxLength": 10,
            "minLength": 3,
            "type": "string"
        },
        "quantity": {
            "default": 1,
            "minimum": 1,
            "type": "integer"
        }
    },
    "required": [
        "prefix"
    ]
}
