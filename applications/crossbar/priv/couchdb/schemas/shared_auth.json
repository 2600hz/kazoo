{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "shared_auth",
    "additionalProperties": false,
    "description": "Provides a local auth-token via a shared auth-token",
    "properties": {
        "shared_auth": {
            "description": "The shared token",
            "maxLength": 64,
            "minLength": 64,
            "type": "string"
        }
    },
    "required": [
        "shared_auth"
    ],
    "type": "object"
}
