{
  "_id": "shared_auth",
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "required": true,
  "name": "Shared Auth",
  "description": "Provides a local auth-token via a shared auth-token",
  "additionalProperties": false,
  "properties": {
    "shared_auth": {
      "required": true,
      "type": "string",
      "name": "Token",
      "description": "The shared token",
      "minLength": 64,
      "maxLength": 64
    }
  }
}
