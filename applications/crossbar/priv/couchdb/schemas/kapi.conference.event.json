            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "conference"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "event"
            ],
            "type": "string"
        },
        "Instance-ID": {
            "type": "string"
        },
        "Participant-ID": {
            "type": "string"
        },
        "Switch-Hostname": {
