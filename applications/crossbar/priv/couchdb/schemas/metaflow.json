{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "_id": "metaflow",
    "additionalProperties": false,
    "properties": {
        "children": {
            "default": {},
            "patternProperties": {
                ".+": {
                    "$ref": "metaflow"
                }
            },
            "required": false,
            "type": "object"
        },
        "data": {
            "default": {},
            "description": "Module Data",
            "required": true,
            "type": "object"
        },
        "module": {
            "maxLength": 15,
            "required": true,
            "type": "string"
        }
    }
}
