{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "kapi.dialplan.set",
    "description": "AMQP API for dialplan.set",
    "properties": {
        "Application-Name": {
            "enum": [
                "set"
            ],
            "type": "string"
        },
        "Call-ID": {
            "type": "string"
        },
        "Custom-Call-Vars": {
            "type": "object"
        },
        "Custom-Channel-Vars": {
            "type": "object"
        },
        "Event-Category": {
            "enum": [
                "call"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "command"
            ],
            "type": "string"
        },
        "Insert-At": {
            "enum": [
                "head",
                "tail",
                "flush",
                "now"
            ],
            "type": "string"
        }
    },
    "required": [
        "Application-Name",
        "Call-ID",
        "Custom-Call-Vars",
        "Custom-Channel-Vars"
    ],
    "type": "object"
}
