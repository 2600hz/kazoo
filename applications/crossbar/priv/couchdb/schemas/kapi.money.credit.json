        "Account-ID": {
            "type": "string"
        },
        "Amount": {
            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "transaction"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "credit"
            ],
            "type": "string"
        },
        "Transaction-ID": {
            "type": "string"
        }
    },
    "required": [
        "Account-ID",
        "Amount",
        "Transaction-ID"
    ],
    "type": "object"
}
