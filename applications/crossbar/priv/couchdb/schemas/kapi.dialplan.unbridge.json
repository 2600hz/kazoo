{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "kapi.dialplan.unbridge",
    "description": "AMQP API for dialplan.unbridge",
    "properties": {
        "Application-Name": {
            "enum": [
                "unbridge"
            ],
            "type": "string"
        },
        "Call-ID": {
            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "call"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "command"
            ],
            "type": "string"
        },
        "Insert-At": {
            "enum": [
                "head",
                "tail",
                "flush",
                "now"
            ],
            "type": "string"
        },
        "Leg": {
            "enum": [
                "A",
                "B",
                "Both"
            ],
            "type": "string"
        }
    },
    "required": [
        "Application-Name",
        "Call-ID"
    ],
    "type": "object"
}
