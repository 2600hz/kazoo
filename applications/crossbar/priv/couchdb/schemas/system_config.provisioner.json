{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "_id": "system_config.provisioner",
    "description": "Schema for provisioner system_config",
    "properties": {
        "cluster_id": {
            "description": "provisioner cluster id",
            "name": "cluster_id",
            "type": "string"
        },
        "feature_keys": {
            "default": {
                "cisco": {
                    "_": {
                        "parking": "",
                        "personal_parking": "",
                        "presence": "",
                        "speed_dial": ""
                    }
                },
                "polycom": {
                    "_": {
                        "parking": "automata",
                        "personal_parking": "automata",
                        "presence": "normal",
                        "speed_dial": "normal"
                    }
                },
                "yealink": {
                    "_": {
                        "parking": "10",
                        "personal_parking": "10",
                        "presence": "16",
                        "speed_dial": "13"
                    }
                }
            },
            "description": "provisioner feature keys",
            "name": "feature_keys",
            "type": "object"
        },
        "provisioning_type": {
            "description": "provisioner provisioning type",
            "name": "provisioning_type"
        },
        "provisioning_url": {
            "description": "provisioner provisioning url",
            "name": "provisioning_url",
            "type": "string"
        }
    },
    "required": true,
    "type": "object"
}
