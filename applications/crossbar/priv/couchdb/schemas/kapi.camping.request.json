{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "kapi.camping.request",
    "description": "AMQP API for camping.request",
    "properties": {
        "Account-ID": {
            "type": "string"
        },
        "Camping-Request": {
            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "camping"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "request"
            ],
            "type": "string"
        }
    },
    "required": [
        "Account-ID",
        "Camping-Request"
    ],
    "type": "object"
}
