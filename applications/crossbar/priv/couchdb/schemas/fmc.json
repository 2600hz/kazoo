{
    "_id": "fmc",
    "description": "FMC configuration file",
    "id": "http://json-schema.org/draft-03/schema#",
    "properties": {
        "x_fmc_header": {
            "description": "FMC Header name (X-FMC-Header for example)",
            "required": true,
            "type": "string"
        },
        "x_fmc_regexp": {
            "description": "Regexp used to extract part of the x_fmc_header's value",
            "required": true,
            "type": "string"
        }
    }
}
