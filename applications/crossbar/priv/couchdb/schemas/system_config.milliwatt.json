{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "system_config.milliwatt",
    "description": "Schema for milliwatt system_config",
    "properties": {
        "echo": {
            "default": {
                "caller_id": [],
                "number": [
                    "5555555552"
                ]
            },
            "description": "milliwatt echo",
            "type": "object"
        },
        "route_win_timeout": {
            "default": 3000,
            "description": "milliwatt route win timeout",
            "type": "integer"
        },
        "tone": {
            "default": {
                "caller_id": [],
                "number": [
                    "5555555551"
                ]
            },
            "description": "milliwatt tone",
            "properties": {
                "duration": {
                    "default": 30000,
                    "description": "milliwatt tone duration",
                    "type": "integer"
                },
                "frequencies": {
                    "default": [
                        "2600"
                    ],
                    "description": "milliwatt tone frequencies",
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "frequency_off": {
                    "default": 1000,
                    "description": "milliwatt tone frequency_off",
                    "type": "integer"
                },
                "frequency_on": {
                    "default": 5000,
                    "description": "milliwatt tone frequency_on",
                    "type": "integer"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}
