{
  "_id": "list_entries",
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "required": true,
  "description": "Schema for a match list entries",
  "properties": {
    "pattern": {
      "type": "string",
      "required": true,
      "name": "Pattern",
      "description": "Match pattern"
    },
    "firstname": {
      "type": "string",
      "required": false,
      "name": "First name",
      "description": "A friendly firstname",
      "minLength": 1,
      "maxLength": 128
    },
    "lastname": {
      "type": "string",
      "required": false,
      "name": "Last name",
      "description": "A friendly lastname",
      "minLength": 1,
      "maxLength": 128
    },
    "displayname": {
      "type": "string",
      "required": false,
      "name": "Display Name",
      "description": "Display name",
      "minLength": 1,
      "maxLength": 128
    },
    "type": {
      "type": "string",
      "required": false,
      "name": "type",
      "minLength": 1,
      "maxLength": 128
    }
  }
}
