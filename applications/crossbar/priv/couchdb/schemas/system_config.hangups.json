{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "_id": "system_config.hangups",
    "description": "Schema for hangups system_config",
    "properties": {
        "hangups_to_monitor": {
            "default": [
                "WRONG_CALL_STATE",
                "NO_ROUTE_DESTINATION",
                "CALL_REJECT",
                "MANDATORY_IE_MISSING",
                "PROGRESS_TIMEOUT",
                "RECOVERY_ON_TIMER_EXPIRE"
            ],
            "description": "hangups hangups to monitor",
            "name": "hangups_to_monitor",
            "type": "array"
        },
        "ignore_hangup_causes": {
            "default": [
                "NO_ANSWER",
                "USER_BUSY",
                "NO_USER_RESPONSE",
                "LOSE_RACE",
                "ATTENDED_TRANSFER",
                "ORIGINATOR_CANCEL",
                "NORMAL_CLEARING",
                "ALLOTTED_TIMEOUT"
            ],
            "description": "hangups ignore hangup causes",
            "name": "ignore_hangup_causes",
            "type": "array"
        }
    },
    "required": true,
    "type": "object"
}
