{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "kapi.conference.dial",
    "description": "AMQP API for conference.dial",
    "properties": {
        "Account-ID": {
            "type": "string"
        },
        "Application-Name": {
            "enum": [
                "dial"
            ],
            "type": "string"
        },
        "Caller-ID-Name": {
            "type": "string"
        },
        "Caller-ID-Number": {
            "type": "string"
        },
        "Conference-ID": {
            "type": "string"
        },
        "Custom-Application-Vars": {
            "type": "object"
        },
        "Custom-Channel-Vars": {
            "type": "object"
        },
        "Endpoints": {
            "items": {
                "$ref": "kapi.dialplan.bridge_endpoint",
                "type": "object"
            },
            "minItems": 1,
            "type": "array"
        },
        "Event-Category": {
            "enum": [
                "conference"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "command"
            ],
            "type": "string"
        },
        "Outbound-Call-ID": {
            "type": "string"
        },
        "Participant-Flags": {
            "type": "string"
        },
        "Profile-Name": {
            "type": "string"
        },
        "Target-Call-ID": {
            "type": "string"
        },
        "Timeout": {
            "type": "integer"
        }
    },
    "required": [
        "Application-Name",
        "Conference-ID",
        "Endpoints"
    ],
    "type": "object"
}
