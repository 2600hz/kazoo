{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "kapi.metaflow.bind",
    "description": "AMQP API for metaflow.bind",
    "properties": {
        "Binding-Digit": {
            "enum": [
                "1",
                "2",
                "3",
                "4",
                "5",
                "6",
                "7",
                "8",
                "9",
                "*",
                "0",
                "#"
            ],
            "type": "string"
        },
        "Call": {
            "type": "object"
        },
        "Call-ID": {
            "type": "string"
        },
        "Digit-Timeout": {
            "type": "integer"
        },
        "Endpoint-ID": {
            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "metaflow"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "bind"
            ],
            "type": "string"
        },
        "Listen-On": {
            "enum": [
                "both",
                "self",
                "peer",
                "aleg",
                "bleg"
            ],
            "type": "string"
        },
        "Numbers": {
            "type": "string"
        },
        "Patterns": {
            "type": "string"
        }
    },
    "required": [
        "Call",
        "Call-ID"
    ],
    "type": "object"
}
