{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "_id": "system_config.omnipresence",
    "description": "Schema for omnipresence system_config",
    "properties": {
        "cache_terminated_callid_s": {
            "default": 60,
            "description": "omnipresence cache terminated callid in seconds",
            "type": "integer"
        },
        "expire_check_ms": {
            "default": 1000,
            "description": "omnipresence expire check in milliseconds",
            "type": "integer"
        },
        "expires_fudge_s": {
            "default": 20,
            "description": "omnipresence expires fudge in seconds",
            "type": "integer"
        },
        "modules": {
            "default": [
                "omnip_dialog_amqp",
                "omnip_message_summary_amqp",
                "omnip_presence_amqp"
            ],
            "description": "omnipresence modules",
            "type": "array"
        },
        "subscriptions_sync_enabled": {
            "default": false,
            "description": "omnipresence subscriptions sync enabled",
            "type": "boolean"
        },
        "use_fast_pickup_cookies": {
            "default": true,
            "description": "omnipresence use fast pickup cookies",
            "type": "boolean"
        }
    },
    "required": true,
    "type": "object"
}
