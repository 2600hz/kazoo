{
  "_id": "ubiquit_auth",
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "required": true,
  "name": "Unbiquit Auth",
  "description": "Provides an auth-token via Ubiquiti's SSO",
  "properties": {
    "username": {
      "type": "string",
      "required": true,
      "name": "Username",
      "description": "Ubiquiti SSO Username",
      "minLength": 1,
      "maxLength": 64
    },
    "password": {
      "type": "string",
      "required": true,
      "name": "Password",
      "description": "Ubiquiti SSO Password",
      "minLength": 1,
      "maxLength": 64
    }
  }
}
