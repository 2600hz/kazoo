{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "ubiquiti_auth",
    "description": "Provides an auth-token via Ubiquiti's SSO",
    "properties": {
        "password": {
            "description": "Ubiquiti SSO Password",
            "maxLength": 64,
            "minLength": 1,
            "type": "string"
        },
        "username": {
            "description": "Ubiquiti SSO Username",
            "maxLength": 64,
            "minLength": 1,
            "type": "string"
        }
    },
    "required": [
        "password",
        "username"
    ],
    "type": "object"
}
