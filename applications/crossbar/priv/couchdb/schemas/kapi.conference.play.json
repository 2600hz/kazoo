{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "kapi.conference.play",
    "description": "AMQP API for conference.play",
    "properties": {
        "Application-Name": {
            "enum": [
                "play"
            ],
            "type": "string"
        },
        "Call-ID": {
            "type": "string"
        },
        "Conference-ID": {
            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "conference"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "command"
            ],
            "type": "string"
        },
        "Media-Name": {
            "type": "string"
        },
        "Participant-ID": {
            "type": "string"
        }
    },
    "required": [
        "Application-Name",
        "Conference-ID",
        "Media-Name"
    ],
    "type": "object"
}
