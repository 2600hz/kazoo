{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "_id": "api_auth",
    "additionalProperties": false,
    "description": "Provides an auth-token via an Account API key",
    "name": "API Auth",
    "properties": {
        "api_key": {
            "description": "The Accounts API key",
            "maxLength": 64,
            "minLength": 64,
            "required": true,
            "type": "string"
        }
    },
    "required": true,
    "type": "object"
}
