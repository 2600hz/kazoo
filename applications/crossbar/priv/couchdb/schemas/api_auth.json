{
  "_id": "api_auth",
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "required": true,
  "name": "API Auth",
  "description": "Provides an auth-token via an Account API key",
  "additionalProperties": false,
  "properties": {
    "api_key": {
      "type": "string",
      "required": true,
      "name": "API Key",
      "description": "The Accounts API key",
      "minLength": 64,
      "maxLength": 64
    }
  }
}
