{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "_id": "kapi.navi.push",
    "description": "AMQP API for navi.push",
    "properties": {
        "Account-ID": {
            "type": "string"
        },
        "App-Name": {
            "enum": [
                "mate"
            ],
            "type": "string"
        },
        "Event-Category": {
            "enum": [
                "navi"
            ],
            "type": "string"
        },
        "Event-Name": {
            "enum": [
                "push"
            ],
            "type": "string"
        },
        "Message": {
            "type": "string"
        },
        "Metadata": {
            "type": "string"
        },
        "Push-Topic": {
            "type": "string"
        },
        "User-ID": {
            "type": "string"
        }
    },
    "required": [
        "Account-ID",
        "Message",
        "Push-Topic",
        "User-ID"
    ],
    "type": "object"
}
