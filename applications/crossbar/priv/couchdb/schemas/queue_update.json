{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "_id": "queue_update",
    "description": "Schema for queue update request",
    "name": "Call Update",
    "properties": {
        "action": {
            "description": "Login agent into queue or logout from it",
            "enum": [
                "login",
                "logout"
            ],
            "name": "Action",
            "required": true,
            "type": "string"
        },
        "queue_id": {
            "description": "Which queue login or logout agent?",
            "maxLength": 128,
            "minLength": 1,
            "name": "Queue ID",
            "required": true,
            "type": "string"
        }
    },
    "required": true,
    "type": "object"
}
