   "properties": {
       "numbers": {
           "description": "",
           "type": "array",
           "required": "true"
       },
       "flow": {
           "description": "",
           "type": "object",
           "required": "true",
           "properties": {
               "module": {
                   "type": "string"
               },
               "data": {
                   "type": "object"
               },
               "children": {
                   "type": "object"
               }
           }
       }
   }
}
