{default_text_template, <<"The first {{event}} was detected on an account.\n\nAccount\nAccount Name: {{account.name}}\nAccount Realm: {{account.realm}}\nAccount ID: {{account.id}}\n\nNumbers\n{% for number in account.pvt_wnm_numbers %}{{number}}\n{% endfor %}\nAdmin User\nFirst Name: {{admin.first_name}}\nLast Name: {{admin.last_name}}\nTimezone: {{admin.timezone}}\nEmail: {{admin.email}}\n\nService\nURL: {{service.url}}\nName: {{service.name}}\nProvider: {{service.provider}}\n\nsent from host: {{service.host}}">>}.
{default_html_template, <<"<html><body><h2>The first {{event}} was detected on an account.</h2><h3>Account:</h3><table cellpadding=\"4\" cellspacing=\"0\" border=\"0\"><tr><td>Account Name:</td><td>{{account.name}}</td></tr><tr><td>Account Realm:</td><td>{{account.realm}}</td></tr><tr><td>Account ID:</td><td>{{account.id}}</td></tr></table><h3>Numbers:</h3><ul>{% for number in account.pvt_wnm_numbers %}<li>{{number}}</li>{% endfor %}</ul><h3>Admin User:</h3><table cellpadding=\"4\" cellspacing=\"0\" border=\"0\"><tr><td>First Name:</td><td>{{admin.first_name}}</td></tr><tr><td>Last Name:</td><td>{{admin.last_name}}</td></tr><tr><td>Timezone:</td><td>{{admin.timezone}}</td></tr><tr><td>Email:</td><td>{{admin.email}}</td></tr></table><h3>Service:</h3><table cellpadding=\"4\" cellspacing=\"0\" border=\"0\"><tr><td>URL:</td><td>{{service.url}}</td></tr><tr><td>Name:</td><td>{{service.name}}</td></tr><tr><td>Service Provider:</td><td>{{service.provider}}</td></tr></table><p> sent from host: {{service.host}}</p></body></html>">>}.
{default_subject_template, <<"First {{event}} on {{account.name}}">>}.
%% {default_from, <<>>}.
